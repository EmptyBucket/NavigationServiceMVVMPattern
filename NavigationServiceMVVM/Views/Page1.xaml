<Page x:Class="NavigationServiceMVVM.Views.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NavigationServiceMVVM.Views"
      mc:Ignorable="d"
      xmlns:viewModel="clr-namespace:NavigationServiceMVVM.ViewModel"
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page1" d:DataContext="{d:DesignInstance viewModel:ViewModelOne}">

    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition />
        <RowDefinition />
      </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="Первая страница" />
      <Button Grid.Row="1" Command="{Binding GoNextPage}" Content="Go Next" />
    </Grid>
</Page>







